<CodeSnippet Format="1.0.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>er IEnumerator(Of T) variable</Title>
    <Shortcut>u</Shortcut>
    <Description>IEnumerator(Of T) variable declaration.</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
    <Keywords>
      <Keyword>Meta-NonUniqueShortcut</Keyword>
      <Keyword>Meta-TitleStartsWithShortcut</Keyword>
    </Keywords>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>_collectionIdentifier</ID>
        <ToolTip>Collection identifier name</ToolTip>
        <Default>items</Default>
      </Literal>
      <Literal>
        <ID>enumerator</ID>
        <ToolTip>Enumerator identifier name</ToolTip>
        <Default>en</Default>
      </Literal>
    </Declarations>
    <Code Language="VB"><![CDATA[Using $enumerator$ = $_collectionIdentifier$.GetEnumerator()
	While $enumerator$.MoveNext()
		$end$
	End While
End Using]]></Code>
  </Snippet>
</CodeSnippet>