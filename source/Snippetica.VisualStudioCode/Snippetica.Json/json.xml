      <Title>array</Title>
      <Shortcut>a</Shortcut>
      <Description>array</Description>
      <Author>Josef Pihrt</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
        <SnippetType>SurroundsWith</SnippetType>
      </SnippetTypes>
      <Keywords>
        <Keyword>Meta-Name Array</Keyword>
      </Keywords>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>name</ID>
          <ToolTip>Name</ToolTip>
          <Default>name</Default>
        </Literal>
      </Declarations>
      <Code Language="Json"><![CDATA["$name$": [ $selected$$end$ ]]]></Code>
    </Snippet>
