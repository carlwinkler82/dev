<CodeSnippet Format="1.0.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>i if not Int32 TryParse</Title>
    <Shortcut>ifftp</Shortcut>
    <Description>if statement with condition whether Int32 TryParse method returns false.</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
    <Keywords>
      <Keyword>Meta-Tag AutoGenerated</Keyword>
      <Keyword>Meta-Tag ExcludeFromReadme</Keyword>
      <Keyword>Meta-Tag NonUniqueShortcut</Keyword>
      <Keyword>Meta-Tag TitleStartsWithShortcut</Keyword>
    </Keywords>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>result</ID>
        <ToolTip>The result</ToolTip>
        <Default>x</Default>
      </Literal>
      <Literal>
        <ID>value</ID>
        <ToolTip>The value to parse</ToolTip>
        <Default>value</Default>
      </Literal>
    </Declarations>
    <Code Language="CSharp"><![CDATA[int $result$;
if (!int.TryParse($value$, out $result$))
{
    $end$
}]]></Code>
  </Snippet>
</CodeSnippet>