<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>@type@ function</Title>
    <Shortcut>f</Shortcut>
    <Description>@type@ function declaration</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
    <Keywords>
      <Keyword>Meta-GenerateParameters</Keyword>
      <Keyword>Meta-GenerateType</Keyword>
      <Keyword>Meta-GenerateVoidType</Keyword>
    </Keywords>
  </Header>
  <Snippet>
    <Declarations>
      <Literal Editable="false">
        <ID>_parameterList</ID>
        <Default>()</Default>
      </Literal>
      <Literal>
        <ID>_type</ID>
        <ToolTip>Return type</ToolTip>
        <Default>void</Default>
      </Literal>
      <Literal>
        <ID>name</ID>
        <ToolTip>Function name</ToolTip>
        <Default>FunctionName</Default>
      </Literal>
    </Declarations>
    <Code Language="Cpp"><![CDATA[$_type$ $name$$_parameterList$
{
	$end$
}]]></Code>
  </Snippet>
</CodeSnippet>